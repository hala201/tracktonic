{
	"compilerOptions": {
	  "module": "NodeNext",
	  "target": "ESNext",
	  "lib": ["ESNext", "DOM"],
	  "outDir": "./out",
	  "rootDir": "./src",
	  "strict": true,
	  "moduleResolution": "NodeNext",
