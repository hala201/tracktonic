{
	"compilerOptions": {
	  "module": "NodeNext",
	  "target": "ESNext",
	  "lib": ["ESNext", "DOM"],
	  "outDir": "./out",
	  "rootDir": "./src",
	  "strict": true,
	  "moduleResolution": "NodeNext",
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "skipLibCheck": true,
	  "strictPropertyInitialization": false,
	},
	"include": ["src", "test"],
  	"exclude": ["node_modules", "out"]
  }